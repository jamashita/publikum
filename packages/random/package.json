{
  "name": "@jamashita/publikum-random",
  "version": "1.1.24",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:jamashita/publikum.git"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": "npm is not supported"
  },
  "private": false,
  "sideEffects": false,
  "engineStrict": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf dist",
    "update": "yarn update:upgrade && yarn update:install",
    "update:upgrade": "ncu --upgrade --timeout 600000",
    "update:install": "yarn install"
  },
  "dependencies": {
    "chance": "1.1.6"
  },
  "devDependencies": {
    "@types/chance": "1.1.0",
    "@types/node": "14.0.5",
    "lint-staged": "10.2.6",
    "npm-check-updates": "6.0.1",
    "rimraf": "3.0.2",
    "typescript": "3.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "b3fd339634e81f85d90315668230c0279c33dd3d"
}
