{
  "name": "@publikum/error",
  "version": "1.1.13",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "exports": {
    "require": "dist/cjs/index.js",
    "import": "dist/esm/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:jamashita/publikum.git"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": "npm is not supported"
  },
  "private": false,
  "sideEffects": false,
  "engineStrict": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf dist",
    "update": "yarn update:upgrade && yarn update:install",
    "update:upgrade": "ncu --upgrade --timeout 600000",
    "update:install": "yarn install"
  },
  "dependencies": {
    "make-error-cause": "2.3.0"
  },
  "devDependencies": {
    "@types/node": "14.0.5",
    "lint-staged": "10.2.6",
    "npm-check-updates": "6.0.1",
    "rimraf": "3.0.2",
    "typescript": "3.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "e14ae5b8526a5325759bc7b4feb0977cb059e139"
}
