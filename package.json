{
  "name": "publikum",
  "version": "0.8.12",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js",
      "default": "./esm/index.js"
    },
    "./AJAX": {
      "require": "./cjs/AJAX/index.js",
      "import": "./esm/AJAX/index.js",
      "default": "./esm/AJAX/index.js"
    },
    "./Cache": {
      "require": "./cjs/Cache/index.js",
      "import": "./esm/Cache/index.js",
      "default": "./esm/Cache/index.js"
    },
    "./Collection": {
      "require": "./cjs/Collection/index.js",
      "import": "./esm/Collection/index.js",
      "default": "./esm//Collection/index.js"
    },
    "./Digest": {
      "require": "./cjs/Digest/index.js",
      "import": "./esm/Digest/index.js",
      "default": "./esm/Digest/index.js"
    },
    "./Error": {
      "require": "./cjs/Error/index.js",
      "import": "./esm/Error/index.js",
      "default": "./esm/Error/index.js"
    },
    "./Interface": {
      "require": "./cjs/Interface/index.js",
      "import": "./esm/Interface/index.js",
      "default": "./esm/Interface/index.js"
    },
    "./JSON": {
      "require": "./cjs/JSON/index.js",
      "import": "./esm/JSON/index.js",
      "default": "./esm/JSON/index.js"
    },
    "./Mock": {
      "require": "./cjs/Mock/index.js",
      "import": "./esm/Mock/index.js",
      "default": "./esm/Mock/index.js"
    },
    "./Object": {
      "require": "./cjs/Object/index.js",
      "import": "./esm/Object/index.js",
      "default": "./esm/Object/index.js"
    },
    "./Quantum": {
      "require": "./cjs/Quantum/index.js",
      "import": "./esm/Quantum/index.js",
      "default": "./esm/Quantum/index.js"
    },
    "./Random": {
      "require": "./cjs/Random/index.js",
      "import": "./esm/Random/index.js",
      "default": "./esm/Random/index.js"
    },
    "./Redis": {
      "require": "./cjs/Redis/index.js",
      "import": "./esm/Redis/index.js",
      "default": "./esm/Redis/index.js"
    },
    "./Superposition": {
      "require": "./cjs/Superposition/index.js",
      "import": "./esm/Superposition/index.js",
      "default": "./esm/Superposition/index.js"
    },
    "./Type": {
      "require": "./cjs/Type/index.js",
      "import": "./esm/Type/index.js",
      "default": "./esm/Type/index.js"
    },
    "./UUID": {
      "require": "./cjs/UUID/index.js",
      "import": "./esm/UUID/index.js",
      "default": "./esm/UUID/index.js"
    },
    "./Zeit": {
      "require": "./cjs/Zeit/index.js",
      "import": "./esm/Zeit/index.js",
      "default": "./esm/Zeit/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:jamashita/publikum.git"
  },
  "author": "jamashita <jamashita@arachnid.xyz>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node ./cjs/index.js",
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "concurrently \"npm:clean:*\"",
    "clean:cjs": "rimraf cjs",
    "clean:esm": "rimraf esm",
    "test": "jest test --detectOpenHandles --coverage",
    "lint": "eslint --fix './src/**/*.{ts,tsx}'",
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "update": "ncu -u && yarn install && yarn upgrade",
    "prepare": "yarn build"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.14"
  },
  "sideEffects": false,
  "engineStrict": true,
  "files": [
    "cjs",
    "esm"
  ],
  "dependencies": {
    "bcrypt": "4.0.1",
    "chance": "1.1.5",
    "dayjs": "1.8.27",
    "ioredis": "4.17.1",
    "make-error-cause": "2.3.0",
    "mysql": "2.18.1",
    "object-hash": "2.0.3",
    "superagent": "5.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.1.0",
    "@types/ioredis": "4.16.2",
    "@types/jest": "25.2.3",
    "@types/mysql": "2.15.11",
    "@types/node": "14.0.4",
    "@types/object-hash": "1.3.3",
    "@types/sinon": "9.0.3",
    "@types/superagent": "4.1.7",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "@typescript-eslint/typescript-estree": "2.34.0",
    "concurrently": "5.2.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.4",
    "npm-check-updates": "6.0.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "ts-jest": "26.0.0",
    "typescript": "3.9.3"
  }
}
