{
  "name": "@jamashita/publikum",
  "version": "2.15.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/publikum.git"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "sideEffects": false,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "lerna run --parallel build",
    "clean": "rimraf coverage && rimraf docs && lerna run --parallel clean",
    "commit": "git cz",
    "documentation": "typedoc --includeVersion --exclude 'node_modules' --exclude '**/*.spec.ts' --stripInternal --out docs .",
    "format": "eslint --fix 'packages/*/src/**/*.{ts,tsx}'",
    "release": "semantic-release",
    "reset": "lerna clean",
    "setup": "lerna bootstrap && lerna link",
    "test": "jest test --detectOpenHandles --passWithNoTests",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade && lerna run --parallel update:install",
    "update:upgrade": "ncu --upgrade --timeout 600000 && lerna run --parallel update:upgrade"
  },
  "dependencies": {
    "axios": "0.20.0",
    "bcrypt": "5.0.0",
    "chance": "1.1.7",
    "dayjs": "1.9.0",
    "got": "11.7.0",
    "ioredis": "4.17.3",
    "make-error-cause": "2.3.0",
    "mysql": "2.18.1",
    "object-hash": "2.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@jamashita/eslint-config": "1.2.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.1.1",
    "@semantic-release/npm": "7.0.6",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.1.0",
    "@types/ioredis": "4.17.4",
    "@types/jest": "26.0.14",
    "@types/mysql": "2.15.15",
    "@types/node": "14.11.2",
    "@types/object-hash": "1.3.3",
    "@types/sinon": "9.0.6",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "@typescript-eslint/typescript-estree": "4.2.0",
    "concurrently": "5.3.0",
    "eslint": "7.10.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.0.2",
    "git-cz": "4.7.1",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "lerna": "3.22.1",
    "lint-staged": "10.4.0",
    "nock": "13.0.4",
    "npm-check-updates": "9.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.1.2",
    "sinon": "9.0.3",
    "sort-package-json": "1.46.0",
    "ts-jest": "26.4.0",
    "typedoc": "0.19.2",
    "typescript": "4.0.3"
  },
  "engines": {
    "node": ">= 14.5.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true
}
