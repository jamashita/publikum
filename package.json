{
  "name": "publikum",
  "version": "0.7.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "dist/index.js",
    "./AJAX": "dist/AJAX/index.js",
    "./Cache": "dist/Cache/index.js",
    "./Collection": "dist/Collection/index.js",
    "./Mock": "dist/Mock/index.js",
    "./MySQL": "dist/MySQL/index.js",
    "./Object": "dist/Object/index.js",
    "./Quantum": "dist/Quantum/index.js",
    "./Redis": "dist/Redis/index.js",
    "./Superposition": "dist/Superposition/index.js",
    "./Type": "dist/Type/index.js",
    "./UUID": "dist/UUID/index.js",
    "./Zeit": "dist/Zeit/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:jamashita/publikum.git"
  },
  "author": "jamashita <jamashita@arachnid.xyz>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/cjs/index.js",
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "clean": "rimraf dist",
    "fix": "concurrently \"npm:fix:*\"",
    "test": "jest test --detectOpenHandles --coverage --config jest.cjs",
    "lint": "eslint --fix 'src/**/*.ts'",
    "fix:lint": "yarn lint",
    "format": "prettier --write src/**/*.ts",
    "fix:format": "yarn format",
    "update": "ncu -u && yarn install && yarn upgrade",
    "prepare": "tsc"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.14"
  },
  "engineStrict": true,
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "bcrypt": "4.0.1",
    "chance": "1.1.4",
    "dayjs": "1.8.26",
    "ioredis": "4.16.3",
    "make-error-cause": "2.3.0",
    "mysql": "2.18.1",
    "object-hash": "2.0.3",
    "superagent": "5.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.0.10",
    "@types/ioredis": "4.16.1",
    "@types/jest": "25.2.1",
    "@types/mysql": "2.15.10",
    "@types/node": "13.13.5",
    "@types/object-hash": "1.3.1",
    "@types/sinon": "9.0.0",
    "@types/superagent": "4.1.7",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "@typescript-eslint/typescript-estree": "2.31.0",
    "concurrently": "5.2.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.2",
    "npm-check-updates": "5.0.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "ts-jest": "25.5.1",
    "typescript": "3.8.3"
  }
}
