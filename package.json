{
  "name": "publikum",
  "version": "0.9.1",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "license": "MIT",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "type": "module",
  "exports": {
    "require": "./cjs/index.js",
    "import": "./esm/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:jamashita/publikum.git"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": "npm is not supported"
  },
  "private": true,
  "sideEffects": false,
  "engineStrict": true,
  "files": [
    "cjs",
    "esm"
  ],
  "scripts": {
    "start": "node ./cjs/index.js",
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "concurrently \"npm:clean:*\"",
    "clean:cjs": "rimraf cjs",
    "clean:esm": "rimraf esm",
    "test": "jest test --detectOpenHandles --coverage",
    "lint": "eslint --fix './src/**/*.{ts,tsx}'",
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "update": "ncu -u && yarn install && yarn upgrade",
    "prepare": "yarn build"
  },
  "dependencies": {
    "bcrypt": "4.0.1",
    "chance": "1.1.5",
    "dayjs": "1.8.27",
    "ioredis": "4.17.1",
    "make-error-cause": "2.3.0",
    "mysql": "2.18.1",
    "object-hash": "2.0.3",
    "superagent": "5.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.1.0",
    "@types/ioredis": "4.16.2",
    "@types/jest": "25.2.3",
    "@types/mysql": "2.15.12",
    "@types/node": "14.0.5",
    "@types/object-hash": "1.3.3",
    "@types/sinon": "9.0.4",
    "@types/superagent": "4.1.7",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "@typescript-eslint/typescript-estree": "3.0.0",
    "concurrently": "5.2.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.1",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lerna": "3.21.0",
    "lint-staged": "10.2.6",
    "npm-check-updates": "6.0.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sinon": "9.0.2",
    "ts-jest": "26.0.0",
    "typescript": "3.9.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
